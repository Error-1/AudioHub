PlayButton.Text = "PLAY"
PlayButton.TextColor3 = Color3.fromRGB(135, 90, 0)
PlayButton.TextSize = 25.000
PlayButton.TextWrapped = true
PlayButton.MouseButton1Click:Connect(function()
	if game.PlaceId == 455366377 or game.PlaceId == 4669040 then
		for i,v in pairs(_player.b():GetChildren()) do
			if (v:IsA'Tool' and v.Name == "Boombox" or v.Name == "BoomBox" or v.Name == "SuperFlyGoldBoombox" or v.Name == "Radio") then
				v.Parent = _player.c()
				local tool = _player.c():FindFirstChildOfClass("Tool")
				if tool then
					tool:FindFirstChildOfClass("RemoteEvent"):FireServer("play",encrypt(InputAudBox.Text))
				end
				return
			end
		end
	elseif game.PlaceId == 177052655 then
		game.ReplicatedStorage.Event:FireServer("PlayRadio", game.Players[_player.p.Name], encrypt(InputAudBox.Text))
	elseif game.PlaceId ~= 177052655 or game.PlaceId ~= 455366377 or game.PlaceId ~= 4669040 then
		for i,v in pairs(_player.b():GetChildren()) do
			if (v:IsA'Tool' and v.Name == "Boombox" or v.Name == "BoomBox" or v.Name == "SuperFlyGoldBoombox" or v.Name == "Radio") then
				v.Parent = _player.c()
				local tool = _player.c():FindFirstChildOfClass("Tool")
				if tool then
					tool:FindFirstChildOfClass("RemoteEvent"):FireServer("PlaySong",encrypt(InputAudBox.Text))
				end
				return
			end
		end
	end
end)

