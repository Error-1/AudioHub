TextButton.Parent = LogFrame
TextButton.BackgroundColor3 = Color3.fromRGB(31, 31, 31)
TextButton.BorderSizePixel = 0
TextButton.Position = UDim2.new(0.363043487, 0, 0.299145311, 0)
TextButton.Size = UDim2.new(0, 124, 0, 31)
TextButton.Font = Enum.Font.Jura
TextButton.Text = "Decode URL"
TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
TextButton.TextSize = 20.000
TextButton.MouseButton1Click:Connect(function()
	local decodeURI
	do
		local char, gsub, tonumber = string.char, string.gsub, tonumber
		local function _(hex) return char(tonumber(hex, 16)) end

		function decodeURI(s)
			s = gsub(s, '%%(%x%x)', _)
			return s
		end
	end
	local player = GetPlayer(Victim1.Text)
	local c = player.Character
	local b = player.Backpack
	if player then
		if c then
			for i,v in pairs(c:GetDescendants()) do
				if(v:IsA'Sound') then
					if v.Name ~= "GettingUp" and v.Name ~= "Died" and v.Name ~= "FreeFalling" and v.Name ~= "Jumping" and v.Name ~= "Landing" and v.Name ~= "Splash" and v.Name ~= "Running" and v.Name ~= "Swimming" and v.Name ~= "Climbing" then
						output.Text = tostring(decodeURI(v.SoundId))
						setclipboard(output.Text)
					end
				end
			end
		end
		for i,v in pairs(b:GetDescendants()) do
			if(v:IsA'Sound') then
				if v.Name ~= "GettingUp" and v.Name ~= "Died" and v.Name ~= "FreeFalling" and v.Name ~= "Jumping" and v.Name ~= "Landing" and v.Name ~= "Splash" and v.Name ~= "Running" and v.Name ~= "Swimming" and v.Name ~= "Climbing" then
					output.Text = tostring(decodeURI(v.SoundId))
					setclipboard(output.Text)
				end
			end
		end
	end
end)

