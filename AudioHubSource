PlayButton2.Name = "PlayButton2"
PlayButton2.Parent = BackpackFrame
PlayButton2.BackgroundColor3 = Color3.fromRGB(255, 170, 0)
PlayButton2.Position = UDim2.new(0.209253937, 0, 0.668085098, 0)
PlayButton2.Size = UDim2.new(0, 200, 0, 50)
PlayButton2.Font = Enum.Font.GothamBold
PlayButton2.Text = "BPLAY"
PlayButton2.TextColor3 = Color3.fromRGB(135, 90, 0)
PlayButton2.TextSize = 25.000
PlayButton2.TextWrapped = true
PlayButton2.MouseButton1Click:Connect(function()
	for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do 
        if v:IsA'Tool' and v.Name == "BoomBox"  or v.Name == "Boombox" or v.Name == "SuperFlyGoldBoombox" then
            v.Parent = game.Players.LocalPlayer.Character
        end
    end
    
    for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do 
        if v:IsA'Tool' and v.Name == "BoomBox"  or v.Name == "Boombox" or v.Name == "SuperFlyGoldBoombox" then
            v:FindFirstChildOfClass'RemoteEvent':FireServer("PlaySong",encrypt(AudBox.Text))
        end
    end
    
    wait(0.6)
    
    game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
    
    wait(0.6)
    
    for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do 
        if v:IsA'Tool' and v.Name == "BoomBox" or v.Name == "Boombox" or v.Name == "SuperFlyGoldBoombox" then
            v.Handle.Sound.TimePosition = TimePosition.Text
            v.Handle.Sound.Playing = true
        end
    end
end)

