if game.SoundService.RespectFilteringEnabled == true then
	RFECheck.Text = "RespectFilteringEnabled: true"
elseif game.SoundService.RespectFilteringEnabled == false then
	RFECheck.Text = "RespectFilteringEnabled: false"
end

rs.Stepped:Connect(function() --servermute
	if servermute then
		for i,v in pairs(_player.all()) do
			if v.Name ~= _player.n() then
				for i,audio in next, v.Character:GetDescendants() do
					if audio:IsA("Sound") then
						audio.Playing = false
					end
				end
			end
		end
	end
end)

antimute = not antimute
rs.Stepped:Connect(function() --antimute
	if antimute then
		if game.PlaceId == 177052655 then
			for i,unmute in pairs(game.Workspace.Ignore.Players[_player.p.Name].Torso:GetDescendants()) do
				if unmute:IsA'Sound' then
					unmute.Playing = true
				end
			end
		end
	elseif game.PlaceId ~= 177052655 then
		for i,v in pairs(game.Workspace[_player.p.Name]:GetDescendants()) do
			if v:IsA'Sound' then
				if v.Name ~= "GettingUp" and v.Name ~= "Died" and v.Name ~= "FreeFalling" and v.Name ~= "Jumping" and v.Name ~= "Landing" and v.Name ~= "Splash" and v.Name ~= "Running" and v.Name ~= "Swimming" and v.Name ~= "Climbing" then
					v.Playing = true
				end
			end
		end
		for i,v in pairs(game.Players.Backpack:GetDescendants()) do
			if v:IsA'Sound' then
				if v.Name ~= "GettingUp" and v.Name ~= "Died" and v.Name ~= "FreeFalling" and v.Name ~= "Jumping" and v.Name ~= "Landing" and v.Name ~= "Splash" and v.Name ~= "Running" and v.Name ~= "Swimming" and v.Name ~= "Climbing" then
					v.Playing = true
				end
			end
		end
	end
end)

rs.Stepped:Connect(function() --player mute
	if playermute then
		local player = GetPlayer(InputNameBox.Text) --add the text box here
		if player then
			for i,v in pairs(player:GetDescendants()) do
				if v:IsA'Sound' and v.Name ~= _player.p.Name then
					local c = player.Character
					for i,audio in next, c:GetDescendants() do
						if audio:IsA("Sound") then
							audio.Playing = false
						end
					end
				end
			end
			for i,v in pairs(player.Backpack:GetDescendants()) do
				if v:IsA'Sound' and v.Name ~= _player.p.Name then
					local c = player.Character
					for i,audio in next, c:GetDescendants() do
						if audio:IsA("Sound") then
							audio.Playing = false
						end
					end
				end
			end
		end
	end
end)

dupe = false
rs = game:GetService("RunService") --dupe system thats really bad
rs.Stepped:Connect(function()
	if dupe then
		workspace[_player.p.Name].Humanoid.Health = 0
		local pos = workspace[_player.p.Name].HumanoidRootPart.Position -- grabs your position
		for i,tool in pairs(game.Players.LocalPlayer.Backpack:GetDescendants()) do
			if tool:IsA'Tool' and tool.Name == "Boombox" or tool.Name == "BoomBox" or tool.Name == "Radio" or tool.Name == "SuperFlyGoldBoombox" then
				tool.Parent = game.Players.LocalPlayer.Character
			end
		end
		for i,tool in pairs(workspace[_player.p.Name]:GetDescendants()) do
			if tool:IsA'Tool' then
			    tool.Handle.CFrame = CFrame.new(0,5000,0)
				tool.Parent = workspace
				tool.Handle.CFrame = CFrame.new(pos)--takes the tools position and replaces it with yours
			end
		end
		for i,v in pairs(workspace:GetChildren()) do
		if (v:IsA"Tool" and v.Name == "Boombox" or v.Name == "BoomBox" or v.Name == "SuperFlyGoldBoombox" or v.Name == "BoomboxGearThree" or v.Name == "Radio") then
			if v:FindFirstChild("Handle") then
				repeat
					wait(.5)
					if _player.c() then
				v.Handle.CFrame = _player.c()["HumanoidRootPart"].CFrame
			end
		until v.Parent == _player.c()
            end
         end
     end
end
end)
