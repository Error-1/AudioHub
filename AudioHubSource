rs:Connect(function()
	if grabradios then
		for i,v in pairs(workspace:GetChildren()) do
			if (v:IsA'Tool' and v.Name == "Boombox" or v.Name == "BoomBox" or v.Name == "SuperFlyGoldBoombox" or v.Name == "Radio") then
				wait(latency1.Text)
				v.Handle.CFrame = _player.c()["HumanoidRootPart"].CFrame
			end
		end
	end
end)

rs:Connect(function()
	if dupe then
		for i,v in pairs(_player.b():GetChildren()) do
			if(v:IsA'Tool' and v.Name == "Boombox" or v.Name == "BoomBox") then
				v.Parent = _player.c()
			end
		end
		for i,v in pairs(_player.c():GetDescendants()) do
			if(v:IsA'Tool' and v.Name == "Boombox" or v.Name == "BoomBox") then
				v.Parent = workspace
				repeat
					wait()
					_player.c():BreakJoints()
					if _player.c() then
						v.Handle.CFrame = _player.c()["HumanoidRootPart"].CFrame
					end
				until dupe == off
			end
		end
	end
end)

rs:Connect(function() --servermute
	if servermute then
		for i,v in pairs(_player.all()) do
			if v.Name ~= _player.n() then
				for i,audio in next, v.Character:GetDescendants() do
					if audio:IsA("Sound") then
						audio.Playing = false
					end
				end
			end
		end
		for i,v in pairs(_player.all()) do
			if v.Name ~= _player.n() then
				for i,audio in next, v.Backpack:GetDescendants() do
					if audio:IsA("Sound") then
						audio.Playing = false
					end
				end
			end
		end
	end
end)

rs:Connect(function()
	if playermute then
		local player = GetPlayer(Victim2.Text)
		local c = player.Character
		local b = player.Backpack
		if player then
			if c then
				for i,v in pairs(c:GetDescendants()) do
					if(v:IsA'Sound') then
						if v.Name ~= "GettingUp" and v.Name ~= "Died" and v.Name ~= "FreeFalling" and v.Name ~= "Jumping" and v.Name ~= "Landing" and v.Name ~= "Splash" and v.Name ~= "Running" and v.Name ~= "Swimming" and v.Name ~= "Climbing" then
							v.Playing = off
						end
					end
				end
			end
		end
		for i,v in pairs(b:GetDescendants()) do
			if(v:IsA'Sound') then
				if v.Name ~= "GettingUp" and v.Name ~= "Died" and v.Name ~= "FreeFalling" and v.Name ~= "Jumping" and v.Name ~= "Landing" and v.Name ~= "Splash" and v.Name ~= "Running" and v.Name ~= "Swimming" and v.Name ~= "Climbing" then
					v.Playing = off
				end
			end
		end
	end
end)
