Victim1.Parent = LogFrame
Victim1.BackgroundColor3 = Color3.fromRGB(31, 31, 31)
Victim1.BorderSizePixel = 0
Victim1.Position = UDim2.new(0.365217388, 0, 0.141025648, 0)
Victim1.Size = UDim2.new(0, 124, 0, 31)
Victim1.Font = Enum.Font.Jura
Victim1.PlaceholderColor3 = Color3.fromRGB(255, 255, 255)
Victim1.PlaceholderText = "Player Name"
Victim1.Text = ""
Victim1.TextColor3 = Color3.fromRGB(255, 255, 255)
Victim1.TextScaled = true
Victim1.TextSize = 20.000
Victim1.TextWrapped = true
Victim1.FocusLost:Connect(function(enter_pressed)
	if enter_pressed then
		local player = GetPlayer(Victim1.Text)
		local c = player.Character
		local b = player.Backpack
		if player then
			if c then
				for i,v in pairs(c:GetDescendants()) do
					if(v:IsA'Sound') then
						if v.Name ~= "GettingUp" and v.Name ~= "Died" and v.Name ~= "FreeFalling" and v.Name ~= "Jumping" and v.Name ~= "Landing" and v.Name ~= "Splash" and v.Name ~= "Running" and v.Name ~= "Swimming" and v.Name ~= "Climbing" then
							setclipboard(tostring(v.SoundId))
							output.Text = v.SoundId
						end
					end
				end
			end
			for i,v in pairs(b:GetDescendants()) do
				if(v:IsA'Sound') then
					if v.Name ~= "GettingUp" and v.Name ~= "Died" and v.Name ~= "FreeFalling" and v.Name ~= "Jumping" and v.Name ~= "Landing" and v.Name ~= "Splash" and v.Name ~= "Running" and v.Name ~= "Swimming" and v.Name ~= "Climbing" then
						setclipboard(tostring(v.SoundId))
						output.Text = v.SoundId
					end
				end
			end
		end
	end
end)

