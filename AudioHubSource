Set.MouseButton1Click:Connect(function()
	local player = GetPlayer(PlayerName.Text)
	if player then
		local c = player.Character
		if c then
			for i,v in pairs(c:GetDescendants()) do
				if v:IsA'Sound' then
					v.TimePosition = TimePosition.Text
				end
			end
			for i,v in pairs(game.Players.LocalPlayer.Backpack:GetDescendants()) do --bplay timeposition for yourself.
				if v:IsA'Sound'then
					v.TimePosition = TimePosition.Text
				end
			end
			for i,v in pairs(player.Backpack:GetDescendants()) do
			    if v:IsA'Sound' then
			        v.TimePosition = TimePosition.Text
			    end
		    end
		end
	end
end)

